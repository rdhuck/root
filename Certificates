<script type="text/javascript">

// Find shortlink URL, then put it into the track page view:
var shorturllink = 'noShortURL';

if ( document.title.indexOf("Site Search") > -1 ) { // See if the title shows that you're on a Search Page.
  shorturllink = 'confluence/dosearchsite.action';
}
else {
  // Parse the links in the head to fetch the shortURL for this page.
  document.head = document.head || document.getElementsByTagName('head')[0]; //The 'or' fetching is needed cuz IE stinks.
  var links = document.head.getElementsByTagName('link');
  for(var link in links){
    if(links.hasOwnProperty(link)){
        var l = links[link];
        if(l.rel === 'shortlink'){
          shorturllink = l.href.slice(18);  // slicing at 18 to take off the "http://kb.mit.edu/" part.
        }
    }
  }
}

  var _gaq = [];
  _gaq.push(['_setAccount', 'UA-16860187-1']);
  _gaq.push(['_trackPageview', shorturllink]);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>

