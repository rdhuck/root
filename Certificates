function initmenu_1() {
    if (window.DynarchMenu) {
        var menuOpts={electric:250, tooltips:true, shadows:[4,6], smoothShadows:true, blink:false, lazy:false, setFocus:false, scrolling:true};
        if (jQuery.browser.opera) menuOpts["scrolling"]=false;
        document.getElementById("menu1-data").className="";
        DynarchMenu.setup("menu1-data", menuOpts);
    }
}

AJS.toInit(function() {window.setTimeout(initmenu_1,0)});

