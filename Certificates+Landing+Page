<script type="text/javascript">
var xmlhttp = undefined;
function processResults()
{
  if (xmlhttp.readyState == 4 && xmlhttp.status == 200)
    {
        document.getElementById('result').innerHTML= xmlhttp.responseText;
    }
}

// Only ever gets called if someone clicks
// 'inaccurate' or 'obsolete' as feedback.
// Sends button and comment to back end.
function processBadComment(buttonid)
{
      var comment = document.forms['take_comment'].elements["feedback_explanation"].value;
      var explanation = comment;
      $('thanks2').innerHTML = '<img src="/confluence/plugins/servlet/builder/resource/KB/icon_green_check.png" style="float:left;margin-top:10px;" alt=""/><span style="float:left; margin-top:10px;">&nbsp;Thanks again!</span>';
      xmlhttp = new XMLHttpRequest();
      var url = "/cgi-bin/feedback.pl?feedback=" + buttonid +"&comment="+explanation;
      xmlhttp.open("GET", url, true);
      xmlhttp.onreadystatechange = processResults;
      xmlhttp.send();
}

// Sends all positive/ok feedback. Not negative, which happens above.
// Resets buttons to thank you messages.
// Serves up comment box if more info is needed.
function sendFeedback(buttonid, blocknum) {
    if (blocknum == 1)
    {
      $('label1').style.display = 'none';
      $('easy').style.display = 'none';
      $('average').style.display = 'none';
      $('difficult').style.display = 'none';
      $('thanks1').style.display = 'block';
    }
    else
    {
       if (buttonid == 'inaccurate' || buttonid == 'obsolete')
       {
         $('thanks2').innerHTML = '<img src="/confluence/plugins/servlet/builder/resource/KB/icon_green_check.png" style="float:left;margin-top:10px;" alt=""/><span style="float:left; margin-top:10px;">&nbsp;Thank you for helping us improve the Knowledge Base.<br/>Please elaborate on how this article is ' + buttonid +'.<br/>Maintainers will be notified about your feedback.</span><div style="clear:both;"></div><form id="take_comment" action="#"><div style="float:left; margin:10px 10px 10px 10px;"><input name="feedback_explanation" type="text" size=110" max="200"/></div><div class="fbbutton" id="comment" onclick="processBadComment(\''+buttonid+'\');">Send Feedback</div></form>';
        }
       $('label2').style.display = 'none';
       $('helpful').style.display = 'none';
       $('inaccurate').style.display = 'none';
       $('obsolete').style.display = 'none';
       $('thanks2').style.display = 'block';
    }

    if (!(buttonid == 'inaccurate' || buttonid == 'obsolete'))
    {
      xmlhttp = new XMLHttpRequest();
      var url = "/cgi-bin/feedback.pl?feedback=" + buttonid;
      xmlhttp.open("GET", url, true);
      xmlhttp.onreadystatechange = processResults;
      xmlhttp.send();
    }
}
</script>

<div style="width:100%; height:190px;">
	<span class="fblabel" style="font-weight:bold;">Feedback</span>
	<hr class="fbbreak"/>
	<span class="fblabel" id="label1">This product/service is:</span>
	<div class="fbreply" id="thanks1" style="display:none;"><img src="/confluence/plugins/servlet/builder/resource/KB/icon_green_check.png" style="float:left;margin-top:10px;" alt=""/><span style="float:left; margin-top:10px; margin-bottom:15px;">&nbsp;Thank you for your feedback. If you would like to provide more details, please log in and add a comment below.</span></div>
	<div style="clear:both;"></div>
	<div class="fbbutton" id="easy" onclick="sendFeedback('easy', 1);">Easy to use</div>
	<div class="fbbutton" id="average" onclick="sendFeedback('average', 1);">Average</div>
	<div class="fbbutton" id="difficult" onclick="sendFeedback('difficult', 1);">Difficult to use</div>
    <div style="clear:both;"></div>
	<hr class="fbbreak"/>
	<span class="fblabel" id="label2">This article is:</span>
	<div class="fbreply" id="thanks2" style="display:none;"><img src="/confluence/plugins/servlet/builder/resource/KB/icon_green_check.png" style="float:left;margin-top:10px;" alt=""/><span style="float:left; margin-top:10px;">&nbsp;Thank you for your feedback. If you would like to provide more details, please log in and add a comment below.</span></div>
    <div style="clear:both;"></div>
	<div class="fbbutton" id="helpful" onclick="sendFeedback('helpful', 2);">Helpful</div>
	<div class="fbbutton" id="inaccurate" onclick="sendFeedback('inaccurate', 2);">Inaccurate</div>
	<div class="fbbutton" id="obsolete" onclick="sendFeedback('obsolete', 2);">Obsolete</div>
    <div style="clear:both;"></div>
</div>

